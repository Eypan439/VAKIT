<script>
document.addEventListener('DOMContentLoaded', function() {
  // Function to set up the click event on mirror elements
  function setupMirrorClicks() {
    const mirrorElements = document.querySelectorAll('[mirror]');

    // Add click event listener to all elements with a 'mirror' attribute
    mirrorElements.forEach(function(mirrorElement) {
      mirrorElement.addEventListener('click', function() {
        // Get the value of the 'mirror' attribute
        const mirrorValue = this.getAttribute('mirror');

        // Find all elements with a 'reflection' attribute matching this value
        const reflectionElements = document.querySelectorAll('[reflection="' + mirrorValue + '"]');

        // Trigger a click on each matched 'reflection' element
        reflectionElements.forEach(function(reflectionElement) {
          reflectionElement.click();
        });
      });
    });
  }

  // Initialize the click behavior for mirror elements
  setupMirrorClicks();
});

</script>

